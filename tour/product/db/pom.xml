<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <artifactId>tourgeek-tour-product-reactor</artifactId>
    <groupId>com.tourgeek.tour</groupId>
    <version>0.8.4-SNAPSHOT</version>
  </parent>

  <artifactId>com.tourgeek.tour.product.db</artifactId>
  <name>tourgeek-tour-product-db - Tour product databases</name>
  <packaging>bundle</packaging>

  <properties>
    <jbundle-package-export>
    com.tourgeek.tour.product.*;version=${project.version},
    </jbundle-package-export>
    <jbundle-package-import>
      *;resolution:=optional,
      com.tourgeek.res.tour.product;resolution:=optional,
    </jbundle-package-import>
  </properties>

  <dependencies>
    <dependency>	<!-- Get rid of this dependency -->
      <groupId>com.tourgeek.thin.app</groupId>
      <artifactId>com.tourgeek.thin.app.booking</artifactId>
      <version>${tourgeek-version-range}</version>
    </dependency>

    <dependency>
      <groupId>com.tourgeek.tour</groupId>
      <artifactId>com.tourgeek.tour.base</artifactId>
      <version>${tourgeek-version-range}</version>
    </dependency>
    <dependency>
      <groupId>com.tourgeek.tour</groupId>
      <artifactId>com.tourgeek.tour.genled.db</artifactId>
      <version>${tourgeek-version-range}</version>
    </dependency>
    <dependency>
      <groupId>com.tourgeek.tour</groupId>
      <artifactId>com.tourgeek.tour.acctrec.db</artifactId>
      <version>${tourgeek-version-range}</version>
    </dependency>
    <dependency>
      <groupId>com.tourgeek.tour</groupId>
      <artifactId>com.tourgeek.tour.acctpay.db</artifactId>
      <version>${tourgeek-version-range}</version>
    </dependency>
    <dependency>
      <groupId>com.tourgeek.tour</groupId>
      <version>${tourgeek-version-range}</version>
      <artifactId>com.tourgeek.tour.message</artifactId>
    </dependency>

    <dependency>
      <groupId>com.tourgeek.model</groupId>
      <version>${tourgeek-version-range}</version>
      <artifactId>com.tourgeek.model.tour.product</artifactId>
    </dependency>
    <dependency>
      <groupId>org.jbundle.main</groupId>
      <artifactId>org.jbundle.main.msg</artifactId>
      <version>${jbundle-version-range}</version>
    </dependency>

    <dependency>
      <groupId>com.tourgeek.res</groupId>
      <artifactId>com.tourgeek.res.tour.product</artifactId>
      <version>${tourgeek-version-range}</version>
    </dependency>
  </dependencies>

</project>
